 "html": "<div id=\"header-html\" class=\"hidden-pdf row section-break\">\r\n\r\n<div class=\"letter-head\"><div><img src=\"https://www.excelbd.com/wp-content/uploads/2020/07/ETL-header.jpg\" style=\"width: 861px;\"><br></div></div>\t\t    \r\n    <div class=\"print-heading\" style=\"padding-bottom: 6px !important; margin-bottom: 16px;\">\r\n\t\t<h2 class=\"text-center\">Delivery Challan</h2>\r\n\r\n\t\t<div class=\"2nd-head\">\r\n\t\t\t<p style=\"display: inline-block; text-align: left; width: 49.70%; \"><b>{{ doc.name }}</b></p>\r\n\t\t\t<p style=\"display: inline-block; text-align: right; width: 49.70%;\">{{ doc.get_formatted(\"territory\") }}</p>\r\n\t\t</div> \r\n\r\n\t</div>\r\n\r\n</div>\r\n\r\n\r\n<div class=\"row section-break\" style=\"margin: 0px !important\">\r\n\t<table class=\"col-xs-6\">\r\n\t\t<tr>\r\n\t\t\t<td style=\"font-size: 14px; font-weight: bold; padding: 0px !important;\"><b>Customer</b></td>\r\n\t\t\t<td class=\"text-center\" style=\"padding: 0px 16px !important;\">:</td>\r\n\t\t\t<td style=\"font-size: 14px; font-weight: bold; padding: 0px !important;\">{{ doc.get_formatted(\"customer_name\") }}</td>\r\n\t\t</tr>\r\n\t\t\r\n\t\t{%- if doc.shipping_address -%}\r\n\t\t<tr>\r\n\t\t\t<td style=\"padding: 0px !important;\">Address</td>\r\n\t\t\t<td style=\"padding: 0px 16px !important;\" class=\"text-center\">:</td>\r\n\t\t\t<td style=\"padding: 0px !important;\">{{ doc.get_formatted(\"shipping_address\") }}</td>\r\n\t\t</tr>\t\r\n\t\t{% else %}\r\n\t\t\t<tr>\r\n\t\t\t<td style=\"padding: 0px !important;\">Address</td>\r\n\t\t\t<td style=\"padding: 0px 16px !important;\" class=\"text-center\">:</td>\r\n\t\t\t<td style=\"padding: 0px !important;\">{{ doc.get_formatted(\"address_display\") }}</td>\r\n\t\t    </tr>\r\n\t    {%- endif %}\r\n\t    \r\n\r\n\t\t<tr>\r\n\t\t\t<td style=\"padding: 0px !important;\">Contact</td>\r\n\t\t\t<td style=\"padding: 0px 16px !important;\" class=\"text-center\">:</td>\r\n\t\t\t<td style=\"padding: 0px !important;