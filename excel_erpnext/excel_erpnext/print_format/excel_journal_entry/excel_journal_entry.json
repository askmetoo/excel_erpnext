 "html": "<!DOCTYPE html>\r\n<div id=\"header-html\" class=\"hidden-pdf row section-break\">\r\n\t\t\r\n\t\t<div class=\"letter-head\"><div><img src=\"https://www.excelbd.com/wp-content/uploads/2020/07/ETL-header.jpg\" style=\"width: 861px;\"><br></div></div>\t\t    \r\n\t\t    <div class=\"print-heading\" style=\"padding-bottom: 6px !important; margin-bottom: 16px;\">\r\n\t\t        \r\n\t\t        <div class=\"2nd-head\" style=\"padding-top: 6px !important;\">\r\n    \t\t\t\t<p style=\"display: inline-block; text-align: left; width: 33%; \"><b>{{ doc.name }}</b></p>\r\n    \t\t\t\t{%- if doc.voucher_type == \"Journal Entry\" -%}\r\n    \t\t\t\t    <p style=\"display: inline-block; text-align: center; width: 32%; \">Journal Entry</p>\r\n    \t\t\t\t{%- elif doc.voucher_type == \"Bank Entry\" -%}\r\n    \t\t\t\t    <p style=\"display: inline-block; text-align: center; width: 32%; \">Bank Entry</p>\r\n    \t\t\t\t{%- elif doc.voucher_type == \"Cash Entry\" -%}\r\n    \t\t\t\t    <p style=\"display: inline-block; text-align: center; width: 32%; \">Cash Entry</p>\r\n    \t\t\t\t{%- elif doc.voucher_type == \"Payment Entry\" -%}\r\n    \t\t\t\t    <p style=\"display: inline-block; text-align: center; width: 32%; \">Payment Entry</p>\r\n    \t\t\t\t{%- elif doc.voucher_type == \"Receive Entry\" -%}\r\n    \t\t\t\t    <p style=\"display: inline-block; text-align: center; width: 32%; \">Receive Entry</p>\r\n    \t\t\t\t{%- elif doc.voucher_type == \"Debit Note\" -%}\r\n    \t\t\t\t    <p style=\"display: inline-block; text-align: center; width: 32%; \">JDebit Note</p>\r\n    \t\t\t\t{%- elif doc.voucher_type == \"Credit Note\" -%}\r\n    \t\t\t\t    <p style=\"display: inline-block; text-align: center; width: 32%; \">Credit Note</p>\r\n    \t\t\t\t{%- elif doc.voucher_type == \"Expense Entry\" -%}\r\n    \t\t\t\t    <p style=\"display: inline-block; text-align: center; width: 32%; \">Expense Entry</p>\r\n    \t\t\t\t{%- elif doc.voucher_type == \"Contra Note\" -%}\r\n    \t\t\t\t    <p style=\"display: inline-block; text-align: center; width: 32%; \">Contra Note</p>\r\n    \t\t\t\t{% else %}\r\n    \t\t\t\t<p style=\"display: inline-block; text-align: center; width: 32%; \">Journal Entry</p>\r\n    \t\t\t\t{%- endif %}\r\n                    <p style=\"display: inline-block; text-align: right; width: 33%;\"><b>{{ doc.get_formatted(\"pay_to_recd_from\") }}</b></p>\r\n                </div>\r\n\r\n\t\t\t</div>\r\n\t    \r\n\t\t</div>\r\n\r\n\t\t<!-- Journal Entry -->\r\n\r\n\t\t<div class=\"row section-break\" style=\"margin: 0px !important\">\r\n\t\t\t<table class=\"col-xs-6\">\r\n\t\t\t\t<tr>\r\n\t\t\t\t\t<td style=\"font-size: 14px; padding: 0px !important;\">Territory</td>\r\n\t\t\t\t\t<td class=\"text-center\" style=\"padding: 0px 16px !important;\">:</td>\r\n\t\t\t\t\t<td style=\"font-size: 14px; padding: 0px !important;\" class=\"text-left\">{{ doc.get_formatted(\"excel_territory\") }}</td>\r\n\t\t\t\t</tr>\r\n                <tr>\r\n\t\t\t\t\t<td style=\"padding: 0px !important;\">Created By</td>\r\n\t\t\t\t\t<td style=\"padding: 0px !important;\" class=\"text-center\">:</td>\r\n\t\t\t\t\t<td style=\"padding: 0px !important;\" class=\"text-left\">{{ frappe.get_fullname(doc.owner) }}</td>\r\n\t\t\t\t</tr>\t\t\t\t\r\n\t\t\t</table>\r\n\r\n\t\t\t<table class=\"col-xs-1\">\r\n\t\t\t\t<tr><td></td></tr>\r\n\t\t\t</table>\r\n\r\n\t\t\t<table class=\"col-xs-5\">\r\n\t\t\t\t<tr>\r\n\t\t\t\t\t<td style=\"padding: 0px !important;\" >Posting Date</td>\r\n\t\t\t\t\t<td style=\"padding: 0px !important;\" class=\"text-center\">:</td>\r\n\t\t\t\t\t<td style=\"padding: 0px !important;\" class=\"text-right\">{{ doc.get_formatted(\"posting_date\") }}</td>\r\n\t\t\t\t</tr>\r\n                <tr>\r\n\t\t\t\t\t<td style=\"padding: 0px !important;\">Approved By</td>\r\n\t\t\t\t\t<td style=\"padding: 0px !important;\" class=\"text-center\">:</td>\r\n\t\t\t\t\t<td style=\"padding: 0px !important;\" class=\"text-right\">{{ frappe.get_fullname(doc.modified_by) }}</td>\r\n\t\t\t\t</tr>\t\t\t\t\r\n\t\t\t</table>\r\n        </div>\r\n\r\n\t\t\t<table class=\"table table-bordered\">\r\n\t\t\t\t<tr>\r\n\t\t\t        <th class=\"text-left\">Sl.</th>\r\n\t\t\t        <th class=\"text-center\">Account</th>\r\n\t\t\t        <th class=\"text-center\">Party</th>\r\n\t\t\t        <th class=\"text-center\">Debit</th>\r\n\t\t\t        <th class=\"text-right\">Credit</th>\r\n\t\t\t    </tr>\r\n\t\t    \t{%- for row in doc.accounts -%}\r\n                <tr>\r\n                    <td class=\"text-left\">{{ row.idx }}</td>\r\n                    <td class=\"text-left\">{{ row.account }}</td>\r\n                    <td class=\"text-left\">{{ row.party }}</td>\r\n                    <td class=\"text-right\">{{ row.debit_in_account_currency }}</td>\r\n                    <td class=\"text-right\">{{ row.credit_in_account_currency }}</td>\r\n                </tr>\r\n                {%- endfor -%}\r\n\t\t\t</table>\r\n\r\n\t\t\t<table>\r\n\t\t\t\t    {%- if doc.cheque_no -%}\r\n\t\t\t\t    <tr>\r\n\t\t\t\t\t\t<td style=\"padding: 0px !important; width: 13%\">Cheque/Ref No</td>\r\n\t\t\t\t\t\t<td style=\"padding: 0px 18px !important;\" class=\"text-center\">:</td>  \r\n\t\t\t\t\t\t<td style=\"padding: 0px !important;\">{{ doc.get_formatted(\"cheque_no\") }}</td>\r\n\t\t\t\t\t</tr>\r\n\r\n\t\t\t\t\t<tr>\r\n\t\t\t\t\t\t<td style=\"padding: 0px !important;\">Ref Date</td>\r\n\t\t\t\t\t\t<td style=\"padding: 0px 18px !important;\" class=\"text-center\">:</td>  \r\n\t\t\t\t\t\t<td style=\"padding: 0px !important;\">{{ doc.get_formatted(\"cheque_date\") }}</td>\r\n\t\t\t\t\t</tr>\r\n                    {%- endif -%}\r\n\t\t\t\t\t<tr>\r\n\t\t\t\t\t\t<td style=\"padding: 0px !important;\">Remarks</td>\r\n\t\t\t\t\t\t<td style=\"padding: 0px 18px !important;\" class=\"text-center\">:</td>  \r\n\t\t\t\t\t\t<td style=\"padding: 0px !important;\">{{ doc.get_formatted(\"user_remark\") }}</td>\r\n\t\t\t\t\t</tr>\r\n\t\t\t</table>\r\n\r\n\t\t<footer style=\"padding-top: 30px; padding-bottom: 20px;\">\r\n\r\n\t\t\t<div id=\"\" style=\"display: block !important; order: 1;\">\r\n\t\t\t\t<div class=\"letter-head-footer\">\r\n\t\t\t\t\t<p style=\"text-align: left; display: inline-block; width: 24.70%;\"><b style=\"border-top: 1px solid #000;\">Created By</b></p>\r\n\t\t\t\t\t<p style=\"text-align: left; display: inline-block; width: 24.70%;\"><b style=\"border-top: 1px solid #000;\">Received By</b></p>\r\n\t\t\t\t\t<p style=\"text-align: left; display: inline-block; width: 23.70%;\"><b style=\"border-top: 1px solid #000;\">Checked By</b></p>\r\n\t\t\t\t\t<p style=\"text-align: right; display: inline-block; width: 24.70%;\"><b style=\"border-top: 1px solid #000;\">Approved By</b></p>\r\n\t\t\t\t\t<div>\r\n\t\t\t\t\t\t<img src=\"https://www.excelbd.com/wp-content/uploads/2021/02/SINV-Footer.jpg\" style=\"width: 861px;\">\r\n\t\t\t\t\t</div>\r\n\t\t\t\t</div>\r\n\t\t\t</div>\r\n\r\n\t\t</footer>\r\n\r\n\t\t<div style=\"padding: 5px 0px; border-top: 2px dashed #000;\" class=\"space\"></div>",
