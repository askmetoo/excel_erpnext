 "html": "<div id=\"header-html\" class=\"hidden-pdf row section-break\">\r\n\t\t\r\n\t\t<div class=\"letter-head\"><div><img src=\"https://www.excelbd.com/wp-content/uploads/2020/07/ETL-header.jpg\" style=\"width: 861px;\"><br></div></div>\t\t    \r\n\t\t    <div class=\"print-heading\" style=\"padding-bottom: 6px !important; margin-bottom: 16px;\">\r\n\t\t\t\t<h2 class=\"text-center\">Stock Entry </h2>\r\n\r\n\t\t\t\t<div class=\"2nd-head\">\r\n\t\t\t\t\t<p style=\"display: inline-block; text-align: left; width: 49.70%; \"><b>{{ doc.name }}</b></p>\r\n\t\t\t\t\t<p style=\"display: inline-block; text-align: right; width: 49.70%;\">{{ doc.get_formatted(\"territory\") }}</p>\r\n\t\t\t\t</div> \r\n\t\t\t\t\r\n\t\t\t\t\t\t\r\n\t\t<div>\r\n\t\t    {%- if doc.status == 'Draft' -%}\r\n                <p class=\"text-center\"><b>{{ doc.status }}</b></p>\r\n\t\t    {% else %}\r\n\t        {%- endif %}\r\n\t\t</div>\r\n\r\n\t\t\t</div>\r\n\t    \r\n\t\t</div>\r\n\r\n\r\n\t\t\t<div class=\"row section-break\" style=\"margin: 0px !important\">\r\n\t\t\t\t<table class=\"col-xs-6\">\r\n\t\t\t\t\t<tr>\r\n\t\t\t\t\t\t<td style=\"font-size: 14px; font-weight: bold; padding: 0px !important;\"><b>Purpose</b></td>\r\n\t\t\t\t\t\t<td class=\"text-center\" style=\"padding: 0px 16px !important;\">:</td>\r\n\t\t\t\t\t\t<td style=\"font-size: 14px; font-weight: bold; padding: 0px !important;\">{{ doc.get_formatted(\"purpose\") }}</td>\r\n\t\t\t\t\t</tr>\t\t\t\t\t\r\n\t\t\t\t</table>\r\n\r\n\t\t\t\t<table class=\"col-xs-1\">\r\n\t\t\t\t\t<tr><td></td></tr>\r\n\t\t\t\t</table>\r\n\r\n\t\t\t\t<table class=\"col-xs-5\">\r\n\t\t\t\t\t<tr>\r\n\t\t\t\t\t\t<td style=\"padding: 0px !important;\" >Posting Date</td>\r\n\t\t\t\t\t\t<td style=\"padding: 0px !important;\" class=\"text-center\">:</td>\r\n\t\t\t\t\t\t<td style=\"padding: 0px !important;\" class=\"text-right\">{{ doc.get_formatted(\"posting_date\") }}</td>\r\n\t\t\t\t\t</tr>\t\t\t\t\r\n\t\t\t\t</table>\r\n\t\t\t</div>\r\n\r\n\r\n\t\t<table class=\"table table-bordered\">\r\n\t\t\t<tr>\r\n\t            <th class=\"text-left\">Sl.</th>\r\n\t            <th class=\"text-left\">Target Warehouse</th>\r\n\t            <th class=\"text-center\">Item Name</th>\r\n\t            <th class=\"text-center\">Qty</th>\r\n\t            <th class=\"text-center\">UOM</th>\r\n\t        </tr>\r\n\t        <tr>\r\n\t        \t{%- for row in doc.items -%}\r\n\t        \t<tr>\r\n\t\t            <td class=\"text-left\">{{ row.idx }}</td>\r\n\t\t            <td class=\"text-left\">{{ row.get_formatted(\"t_warehouse\", doc) }}</td>\r\n\t\t            <td class=\"text-left\">\r\n\t\t                {{ row.item_name }}\r\n\t                    {% if row.item_code != row.item_name -%}\r\n\t                    <br>Item Code: {{ row.item_code}}\r\n\t                    {%- endif %}\r\n\t\t            </td>\r\n\t\t            <td class=\"text-right\">{{ row.qty }}</td>\r\n\t\t            <td class=\"text-right\">{{ row.get_formatted(\"uom\", doc) }}</td>\r\n\t\t        </tr>\r\n\t\t        \r\n\t\t                   \t\t\r\n\r\n\t\t\t\t{% if row.item_details  %}\r\n\t\t\t\t<tr>\r\n\t\t            <td colspan=\"2\" class=\"text-left\"> \r\n                        <b>Details: </b>{{ row.item_details }}\r\n\t\t            </td>\r\n\t\t            <td colspan=\"3\"></td>\r\n\t\t        </tr>\r\n            \t{% else %}\r\n           \t\t{%- endif %}\r\n\r\n\t\t\t\t{% if row.serial_no  %}\r\n\t\t\t\t<tr>\r\n\t\t            <td colspan=\"3\" class=\"text-left\"> \r\n                        <b>Serials: </b>{{ row.serial_no }}\r\n\t\t            </td>\r\n\t\t            <td colspan=\"3\"></td>\r\n\t\t        </tr>\r\n            \t{% else %}\r\n           \t\t{%- endif %}\r\n           \t\t\r\n\t\t        {%- endfor -%}\r\n\t\t    </tr>\r\n\t\t</table>\r\n\t\t\r\n\t\t<div>\r\n\t\t    {%- if doc.remarks -%}\r\n                <p class=\"text-left\">Remarks: {{ doc.remarks }}</p>\r\n\t\t    {% else %}\r\n\t        {%- endif %}\r\n\t\t</div>\