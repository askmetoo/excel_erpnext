 "html": "<div id=\"header-html\" class=\"hidden-pdf row section-break\">\r\n\t\t\r\n\t<div class=\"letter-head\"><div><img src=\"https://www.excelbd.com/wp-content/uploads/2020/07/ETL-header.jpg\" style=\"width: 861px;\"><br></div></div>\t\t    \r\n\t\t<div class=\"print-heading\" style=\"padding-bottom: 6px !important; margin-bottom: 3px;\">\r\n\t\t\t\r\n\t\t\t<div class=\"2nd-head\" style=\"padding-top: 6px !important;\">\r\n\t\t\t\t<p style=\"display: inline-block; text-align: left; width: 33%; \">{{ doc.name }}</p>\r\n\t\t\t\t<p style=\"display: inline-block; text-align: center; width: 32%; \"><b>CHALLAN</b></p>\r\n\t\t\t\t<p style=\"display: inline-block; text-align: right; width: 33%;\">{{ doc.get_formatted(\"territory\") }}</p>\r\n\t\t\t</div> \r\n\t\t\t\r\n\t\t\t\t\t\r\n\t<div>\r\n\t\t{%- if doc.status == 'Draft' -%}\r\n\t\t\t<p class=\"text-center\"><b>{{ doc.status }}</b></p>\r\n\t\t{% else %}\r\n\t\t{%- endif %}\r\n\t</div>\r\n\r\n\t\t</div>\r\n\t\r\n\t</div>\r\n\r\n\r\n\t\t<div class=\"row section-break\" style=\"margin: 0px !important\">\r\n\t\t\t<table class=\"col-xs-6\">\r\n\t\t\t\t<tr>\r\n\t\t\t\t\t<td style=\"padding: 0px !important;\">Customer</td>\r\n\t\t\t\t\t<td class=\"text-center\" style=\"padding: 0px 16px !important;\">:</td>\r\n\t\t\t\t\t<td style=\"padding: 0px !important;\">{{ doc.get_formatted(\"customer_name_print\") }}</td>\r\n\t\t\t\t</tr>\r\n\t\t\t\t{%- if doc.address -%}\r\n\t\t\t\t<tr>\r\n\t\t\t\t\t<td style=\"padding: 0px !important;\">Address</td>\r\n\t\t\t\t\t<td style=\"padding: 0px 16px !important;\" class=\"text-center\">:</td>\r\n\t\t\t\t\t<td style=\"padding: 0px !important;\">{{ doc.get_formatted(\"address\") }}</td>\r\n\t\t\t\t</tr>\r\n\t\t\t\t{%- endif %}\r\n\t\t\t\t{%- if doc.attention -%}\r\n\t\t\t\t<tr>\r\n\t\t\t\t\t<td style=\"padding: 0px !important;\">Attention</td>\r\n\t\t\t\t\t<td style=\"padding: 0px 16px !important;\" class=\"text-center\">:</td>\r\n\t\t\t\t\t<td style=\"padding: 0px !important;\">{{ doc.get_f